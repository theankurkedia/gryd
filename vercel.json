{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "framework": "nextjs",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "trailingSlash": true,
  "distDir": ".next",
  "output": "export"
}
